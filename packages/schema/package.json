  },
  "dependencies": {
    "@apexo/type": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "joi": "^18.0.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "zod": "^4.3.5"
  },
