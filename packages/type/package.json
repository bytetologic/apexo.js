{
  "name": "@apexo/type",
  "version": "0.0.1",
  "description": "@apexo/type is a small, zero-dependency library that gives you easy and reliable type checks in JavaScript and TypeScript.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --minify",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "release": "pnpm publish --access public"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "types",
    "typescript",
    "javascript",
    "validation",
    "schema",
    "runtime",
    "type-inference",
    "data-validation",
    "data-contract",
    "static-types",
    "run-time",
    "data-structure",
    "forms",
    "api",
    "browser",
    "server",
    "backend",
    "validation-library"
  ],
  "author": "Lakshay Singh",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bytetologic/apexo.js/tree/main/packages/type"
  }
}